{{!< default}}

<article class="post-full {{post_class}}">
    <header class="post-header">
        <h1 class="post-title y2k-heading">{{title}}</h1>
        {{#if feature_image}}
            <img class="post-feature-image" src="{{feature_image}}" alt="{{title}}">
        {{/if}}
    </header>
    <section class="post-content">
        {{content}}
    </section>
</article> 